<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ssm.tk.mapper.demo.dao.UserDao">
    <select id="selectRolesByName" parameterType="java.lang.String" resultType="java.lang.String">
        SELECT roles FROM user
        WHERE name = #{_parameter,jdbcType=VARCHAR};
    </select>
</mapper>